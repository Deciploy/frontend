{
  "name": "user-auth",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "shared/user-auth/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/shared/user-auth",
        "main": "shared/user-auth/src/index.ts",
        "tsConfig": "shared/user-auth/tsconfig.lib.json",
        "assets": ["shared/user-auth/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "shared/user-auth/**/*.ts",
          "shared/user-auth/package.json"
        ]
      }
    }
  },
  "tags": []
}
